        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAro29"
        app:srcCompat="@drawable/b30" />

    <ImageButton
        android:id="@+id/imageButtonB31"
        android:layout_width="120dp"
        android:layout_height="105dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonB29"
        app:srcCompat="@drawable/b31" />

    <ImageButton
        android:id="@+id/imageButtonB32"
        android:layout_width="120dp"
        android:layout_height="105dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="40dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonB30"
        app:srcCompat="@drawable/b32" />

    <Button
        android:id="@+id/buttonInicioAro29"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="195dp"
        android:layout_marginEnd="100dp"
        android:text="InÃ­cio"
        app:layout_constraintEnd_toEndOf="parent"
